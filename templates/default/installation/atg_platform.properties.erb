-fileOverwrite_<%= node[:atg][:installation_dir] %>/.ASE10.2_config/bin/getlicense.sh=Yes
-fileOverwrite_<%= node[:atg][:installation_dir] %>/.ASE10.2_config/bin/getlicense.bat=Yes
-fileOverwrite_<%= node[:atg][:installation_dir] %>/uninstall/.ASE10.2_uninstall/Uninstall Oracle ATG Web Commerce 10.2.lax=Yes