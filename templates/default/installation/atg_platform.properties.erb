#Installed features
#------------------
CHOSEN_INSTALL_FEATURE_LIST=RM,Commerce,Portal,CA,MP,QF

#Choose Install Folder
#---------------------
USER_INSTALL_DIR=<%= node[:atg][:installation_dir] %>

#Product Selection Console
#-------------------------
CHOSEN_INSTALL_FEATURE_LIST=RM,Commerce,Portal,CA,MP,QF

#Select Application Server
#-------------------------
SELECTED_APP_SERVER=\"\",\"\",\"\",\"Oracle WebLogic\",\"\"

#WebLogic Console
#----------------
ATG_BEA_HOME=<%= node[:atg][:bea_home] %>
ATG_WL_HOME=<%= node[:atg][:weblogic][:installation_dir] %>
ATG_WL_DOMAIN=<%= node[:atg][:weblogic][:domain_path] %>
ATG_LISTEN_PORT=<%= node[:atg][:weblogic][:port] %>
ATG_RMI_PORT=8860
ATG_JDK_HOME=<%= node[:java][:java_home] %>

